<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009"
			   xmlns:s="library://ns.adobe.com/flex/spark"
			   width="1250"
			   height="650"
			   xmlns:mx="library://ns.adobe.com/flex/mx"
			   xmlns:swiz="http://swiz.swizframework.org"
			   xmlns:view="com.vestas.view.*">
	<fx:Style source="assets/myStyle.css"/>
	<fx:Script>
		<![CDATA[
			import com.vestas.bean.Beans;
		]]>
	</fx:Script>
	<fx:Declarations>
		<swiz:SwizConfig strict="true"
						 injectionEvent="preinitialize"
						 mediateBubbledEvents="true"
						 eventPackages="com.vestas.events"
						 viewPackages="com.vestas.view"
						 beanLoaders="{[ Beans ]}"/>

	</fx:Declarations>

	<view:HomeView/>

</s:Application>
